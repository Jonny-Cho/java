package ch11;

import java.util.*;

public class MyStack {

	public Object push(Object item){
		addElement(item);
		return item;
	}
	
	public Object pop(){
		Object obj = peek();
		//
		removeElementAt(size()-1);
		return obj;
	}
	
	public Object peek(){
		int len = size();
		
		if(len==0)
			throw new EmptyStackException();
		//
		return elementAt(len -1);
	}
	
	public static void main(String[] args) {
		
	}

}
