package ch9;

class Circle implements Cloneable {
	Point p;
	double r;
	Circle(Point p, double r) {
		this.p = p;
		this.r = r;
	}
	
	public Circle shallowCopy(){
		Object obj = null;
		try {
			obj = super.clone();
		} catch (CloneNotSupportedException e) {
			return (Circle)obj;
		}
	}
}

public class ShallowDeepCopy {

	public static void main(String[] args) {
		
	}

}
