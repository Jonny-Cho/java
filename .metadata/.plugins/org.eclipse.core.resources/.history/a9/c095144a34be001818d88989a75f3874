package ch7;

class Product {
	int price;
	int bonusPoint;
	
	Product(int price){
		this.price = price;
		bonusPoint = (int)(price/10.0); 
	}
}

class Tv extends Product{
	Tv(){ 
		// 조상클래스 생성자 호출
		super(100);
		public String toString() { return "Tv"; }
	}
}

class Computer extends Product{
	
}


