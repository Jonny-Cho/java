package star;

public class Star7 {

	public static void main(String[] args) {
		for(int i=1; i<=5;i++){
			if(i==1 || i==5){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
		}
		for(int k=1; k<=5;k++){
			if(k==2 || k==4){
					System.out.print("*");
				}
			else{
					System.out.print(" ");
				}
				System.out.println();
	}

}
}
