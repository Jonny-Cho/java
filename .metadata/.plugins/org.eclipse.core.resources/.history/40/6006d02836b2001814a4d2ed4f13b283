package ooptest2;

class Point {
	int x;
	int y;
	
	public String toString(){
		return "x = " + x + "y = " + y
	}
}

public class oop1 {

	public static void main(String[] args) {
		Point p = new Point();
		p.x = 3;
		p.y = 5;
		System.out.println(p.x);
		System.out.println(p.y);
		System.out.println(p.toString());
	}

}
