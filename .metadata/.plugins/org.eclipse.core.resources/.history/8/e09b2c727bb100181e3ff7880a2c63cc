package ch6_prac;

class Ex6_07 {

	class MyPoint {
		int x;
		int y;
		
		MyPoint(int x, int y) {
			this.x = x;
			this.y = y;
		}
		
		double getDistance(int x, int y, int x1, int y1) { // cm
			double result = Math.sqrt((x-x1)*(x-x1)+(y-y1)*(y-y1)); 
			return result;
		}
		
		}
	
	
	
	
	public static void main(String args[]) { //cm -> cm »£√‚
		System.out.println(getDistance(1,1,2,2));
	}
		
}

