package ooptest;

class Point {
	int x;
	int y;

	public String toString() {
		return "x : " + x + ", y : " + y;
	}
	
	Point(){}; // 기본생성자
	Point(int x, int y){
		this.x = x;
		this.y = y;
	}; // 매개변수 있는 생성자
}

public class PointTest {
	public static void main(String[] args) {
		Point p = new Point(2,2);
		Point p = new Point();
		System.out.println("p.x=" + p.x);
		System.out.println("p.y=" + p.y);
		System.out.println(p.toString());
	}
}
