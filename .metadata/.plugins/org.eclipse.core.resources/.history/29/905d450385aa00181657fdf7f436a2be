package star;

public class star8 {

	public static void main(String[] args) {
		// ctrl+D 한줄삭제
		// sysout 입력하고 ctrl+space
		
        for(int i=1; i<=5; i++) {
        	for(int j=1; j<=9; j++){
        		if((i+j>=6) && (j-i<=6)){
        			System.out.print("*");
        		}
        		else {
        			System.out.print(" ");
        		}
        	}
        	System.out.println();
        }
	}

}
