package star;

public class Star7 {

	public static void main(String[] args) {
		for(int i=1; i<=5;i++){
			if(i==1 || i==5){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
			if(i==2 || i==4){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
			if(i==1 || i==5){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
			if(i==3){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
			if(i==2 || i==4){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();
			if(i==1 || i==5){
				System.out.print("*");
			}
			else{
				System.out.print(" ");
			}
			System.out.println();

	}

}
}
