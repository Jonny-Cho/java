package star;

public class star8 {

	public static void main(String[] args) {
		// ctrl+D 한줄삭제
		// sysout 입력하고 ctrl+space
		
		
		for(int i=0;i<=4;i++){
			for(int j=1;j<=9;j++){
				if(j == 2){
					System.out.print("*");
				}
				else
					System.out.println(" ");	
			}
			System.out.println();
		}
		
	}

}
