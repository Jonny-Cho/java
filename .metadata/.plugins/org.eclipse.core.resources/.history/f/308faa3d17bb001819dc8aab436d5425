package Exercise;

import java.awt.*;
import java.awt.event.*;

public class Calc {
	public static void main(String[] args) {
		Frame f = new Frame("계산기");
		f.setSize(150,150);
		f.setLayout(new GridLayout(3, 2)); // 3행 2열
		f.add(new Button("1"));
		f.add(new Button("2"));
		f.add(new Button("3"));
		f.add(new Button("4"));
		f.add(new Button("5"));
		f.add(new Button("6"));
		
		f.setVisible(true);
	}
}
