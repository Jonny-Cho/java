package star;

public class star8 {

	public static void main(String[] args) {
		
		// 7번에서 if로 걸러냄
		
		
		
		// 전체찍고 if로 걸러냄
        for(int i=1; i<=5; i++) {
        	for(int j=1; j<=9; j++){
        		if((i+j>5) && (j-i<5)){
        			System.out.print("*");
        		}
        		else {
        			System.out.print(" ");
        		}
        	}
        	System.out.println();
        }
	}
}
