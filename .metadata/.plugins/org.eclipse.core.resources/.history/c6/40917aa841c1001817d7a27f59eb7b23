package ch12;

class StudentInfo{
	public int grade;
	StudentInfo(int grade){
		this.grade = grade;
	}
}

class StudentPerson{
	public StudentInfo info;
	StudentInfo(int grade){
		this.grade = grade;
	}
}


public class GenericDemo2 {
	public static void main(String[] args) {
		Person <String> p1 = new Person<String>();
		Person <StringBuilder> p2 = new Person<StringBuilder>();
	}
}

