	@Test public void test() {assertTrue(mid(1,3,3)==3);}
	@Test public void test() {assertTrue(mid(3,2,1)==2);}
	@Test public void test() {assertTrue(mid(2,3,1)==2);}
	@Test public void test() {assertTrue(mid(1,1,3)==1);}
	@Test public void test() {assertTrue(mid(2,2,1)==2);}
	@Test public void test() {assertTrue(mid(2,2,2)==2);}
	@Test public void test() {assertTrue(mid(3,3,3)==3);}
	@Test public void test() {assertTrue(mid(2,1,1)==1);}
	@Test public void test() {assertTrue(mid(1,3,2)==2);}
	@Test public void test() {assertTrue(mid(1,2,3)==2);}
	
	int mid(int x, int y, int z){
		return y;
	}

}
